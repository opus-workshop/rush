ert!(err.downcast_ref::<BreakSignal>().is_some());\n   102→        assert_eq!(err.downcast_ref::<BreakSignal>().unwrap().levels, 2);\n   103→        runtime.exit_loop();\n   104→        runtime.exit_loop();\n   105→    }\n   106→\n   107→    #[test]\n   108→    fn test_break_with_zero() {\n   109→        let mut runtime = Runtime::new();\n   110→        runtime.enter_loop();\n   111→        let result = builtin_break(&[\"0\".to_string()], &mut runtime);\n   112→        assert!(result.is_err());\n   113→        assert!(result.unwrap_err().to_string().contains(\"loop count out of range\"));\n   114→        runtime.exit_loop();\n   115→    }\n   116→\n   117→    #[test]\n   118→    fn test_break_with_invalid_number() {\n   119→        let mut runtime = Runtime::new();\n   120→        runtime.enter_loop();\n   121→        let result = builtin_break(&[\"not_a_number\".to_string()], &mut runtime);\n   122→        assert!(result.is_err());\n   123→        assert!(result.unwrap_err().to_string().contains(\"numeric argument required\"));\n   124→        runtime.exit_loop();\n   125→    }\n   126→\n   127→    #[test]\n   128→    fn test_break_too_many_args() {\n   129→        let mut runtime = Runtime::new();\n   130→        runtime.enter_loop();\n   131→        let result = builtin_break(&[\"1\".to_string(), \"2\".to_string()], &mut runtime);\n   132→        assert!(result.is_err());\n   133→        assert!(result.unwrap_err().to_string().contains(\"too many arguments\"));\n   134→        runtime.exit_loop();\n   135→    }\n   136→\n   137→    #[test]\n   138→    fn test_break_exceeds_loop_depth() {\n   139→        let mut runtime = Runtime::new();\n   140→        runtime.enter_loop();\n   141→        let result = builtin_break(&[\"2\".to_string()], &mut runtime);\n   142→        assert!(result.is_err());\n   143→        assert!(result.unwrap_err().to_string().contains(\"loop count out of range\"));\n   144→        runtime.exit_loop();\n   145→    }\n   146→}\n   147→\n\n<sy